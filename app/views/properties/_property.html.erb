<div class="property-card">
  <%= property.public_id%>
  </br>
  <%= link_to property_path(property.public_id) do %>
    <% property.title %>
  <% end %>
  </br>
  <%= property.property_type%>
  </br>
  <%= property.location%>
  </br>
  <% if property.title_image_full%>
    <%= image_tag property.title_image_full, class: "property-image"%>
  <% end %>
  </br>
</div>
</br></br>